{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "eofline": false,
        "interface-name": false,
        "object-literal-sort-keys": false,
        "trailing-comma": false,
        "whitespace": false,
        "member-access": false,
        "curly": false,
        "new-parens": false,
        "triple-equals": false,
        "ordered-imports": false,
        "object-literal-shorthand": false,
        "quotemark": false
    },
    "rulesDirectory": []
}